{
  "name": "github-trending-email",
  "private": "true",
  "version": "0.0.1",
  "description": "Github trending repos send to your email every day",
  "main": "lib/index.js",
  "module": "src/index.ts",
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "ncc build src/index.ts -o lib -m",
    "lint": "eslint src/**",
    "format": "prettier --write src/** action.yml README.md",
    "format:check": "prettier --check src/** action.yml README.md",
    "postinstall": "simple-git-hooks"
  },
  "devDependencies": {
    "@octokit/types": "^12.6.0",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.1.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.10.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12"
  },
  "license": "MIT",
  "keywords": [
    "github-trending",
    "github repo",
    "github email",
    "github action"
  ],
  "simple-git-hooks": {
    "pre-commit": "pnpm run lint && pnpm run format"
  },
  "engines": {
    "node": ">=18"
  }
}
